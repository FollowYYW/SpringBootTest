<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboottest.mapper.EmpMapper">

    <select id="selectEmpListByEmpNo" parameterType="int" resultType="EmpEntity">
        select empNo,eName,job,mgr,to_char(hireDate, 'yyyy-mm-dd HH24:mi:ss'),sal,comm,deptNo from emp where empNo = #{empNo}
    </select>

</mapper>